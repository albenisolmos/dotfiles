struct {ID} {list_block}
{type}[]

ARRAYS
let my_array: int[] = [1,2,3,4]
let my_array: str[] = ['a', 'b', 'c']
let my_array: array = ['a', 2, 'c']
let my_array: array[]
let my_array = ['a', 2, 'c']

Index Operation
let num_array = [1,2,3,4,5,6]
num_array[0] // 1
num_array[5] // 6
num_array[:4] // 1 2 3 4 5
num_array[4:] // 6
num_array[1:2] // 2 3

DICTIONARIES
let dic = {
	loco = 666,
	age = 17,
	code = true
}

print(my_dic['loco'])
print(my_dic.loco)
